#include<stdio.h>
int main(){
      int x,y,z;
     printf("enter x");
      scanf("%d",&x);
     printf("enter y");
     scanf("%d",&y);
      printf(" enter z");
      scanf("%d",&z);
      if(x>y&&x>z){
          printf("x");
      }
      if(y>z&&y>x){
         printf("y");
     }
     if(z>x&&z>y){
          printf("z");
      }
      return 0;
}








